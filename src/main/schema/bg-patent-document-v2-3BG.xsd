<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xml="http://www.w3.org/XML/1998/namespace" elementFormDefault="qualified">
	<xs:import namespace="http://www.w3.org/XML/1998/namespace" />
	<xs:complexType name="math" mixed="true" />
	<xs:element name="math" type="math" />
	<xs:complexType name="table" mixed="true" />
	<xs:element name="table" type="table" />
	<xs:complexType name="bg-patent-document">
		<xs:choice>
			<xs:element ref="doc-page" maxOccurs="unbounded" />
			<xs:sequence>
				<xs:choice>
					<xs:element ref="bibliographic-data" />
					<xs:element ref="SDOBI" />
				</xs:choice>
				<xs:element ref="description" minOccurs="0" />
				<xs:element ref="claims" minOccurs="0" />
				<xs:element ref="amended-claims" minOccurs="0" maxOccurs="unbounded" />
				<xs:element ref="amended-claims-statement" minOccurs="0"
					maxOccurs="unbounded" />
				<xs:element ref="abstract" minOccurs="0" maxOccurs="unbounded" />
				<xs:element ref="drawings" minOccurs="0" />
				<xs:element ref="search-report-data" minOccurs="0"
					maxOccurs="unbounded" />
				<xs:element ref="sequence-list-doc" minOccurs="0" />
				<xs:element ref="table-external-doc" minOccurs="0"
					maxOccurs="unbounded" />
			</xs:sequence>
		</xs:choice>
		<xs:attribute name="id" type="xs:string" />
		<xs:attribute name="lang" type="xs:anySimpleType" use="required" />
		<xs:attribute name="dtd-version" type="xs:anySimpleType" />
		<xs:attribute name="file" type="xs:anySimpleType" />
		<xs:attribute name="country" type="xs:anySimpleType" use="required" />
		<xs:attribute name="file-reference-id" type="xs:anySimpleType" />
		<xs:attribute name="correction-code" type="xs:anySimpleType" />
		<xs:attribute name="doc-number" type="xs:anySimpleType" />
		<xs:attribute name="kind" type="xs:anySimpleType" />
		<xs:attribute name="date-produced" type="xs:anySimpleType" />
		<xs:attribute name="date-publ" type="xs:anySimpleType" />
		<xs:attribute name="status" type="xs:anySimpleType" />
	</xs:complexType>
	<xs:element name="bg-patent-document" type="bg-patent-document" />
	<xs:complexType name="sequence-list-doc" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element ref="doc-page" />
			<xs:element ref="sequence-list" />
		</xs:choice>
		<xs:attribute name="id" type="xs:ID" />
		<xs:attribute name="lang" type="xs:anySimpleType" />
		<xs:attribute name="status" type="xs:anySimpleType" />
	</xs:complexType>
	<xs:element name="sequence-list-doc" type="sequence-list-doc" />
	<xs:complexType name="sequence-list">
		<xs:attribute name="id" type="xs:ID" />
		<xs:attribute name="file" type="xs:anySimpleType" use="required" />
		<xs:attribute name="lang" type="xs:anySimpleType" />
		<xs:attribute name="carriers">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="paper" />
					<xs:enumeration value="internal-electronic" />
					<xs:enumeration value="external-electronic" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="seq-file-type" type="xs:anySimpleType"
			fixed="ST.25" />
		<xs:attribute name="status" type="xs:anySimpleType" />
		<xs:attribute name="electronic-format-type">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="text" />
					<xs:enumeration value="other" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="part-of-application">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="sequence-list" type="sequence-list" />
	<xs:complexType name="search-report-data">
		<xs:choice>
			<xs:element ref="doc-page" maxOccurs="unbounded" />
			<xs:sequence>
				<xs:element ref="srep-info" minOccurs="0" />
				<xs:element ref="srep-for-pub" minOccurs="0" />
				<xs:element ref="srep-written-opinion" minOccurs="0" />
				<xs:element ref="srep-search-opinion" minOccurs="0" />
			</xs:sequence>
		</xs:choice>
		<xs:attribute name="id" type="xs:ID" />
		<xs:attribute name="lang" type="xs:anySimpleType" />
		<xs:attribute name="date-produced" type="xs:anySimpleType"
			use="required" />
		<xs:attribute name="srep-office" type="xs:anySimpleType"
			use="required" />
		<xs:attribute name="srep-type" type="xs:anySimpleType" />
		<xs:attribute name="status" type="xs:anySimpleType" />
	</xs:complexType>
	<xs:element name="search-report-data" type="search-report-data" />
	<xs:complexType name="srep-search-opinion">
		<xs:choice>
			<xs:choice>
				<xs:element ref="doc-page" maxOccurs="unbounded" />
			</xs:choice>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="heading" minOccurs="0" maxOccurs="unbounded" />
				<xs:element ref="p" maxOccurs="unbounded" />
			</xs:sequence>
		</xs:choice>
		<xs:attribute name="id" type="xs:ID" />
		<xs:attribute name="opinion-type" type="xs:anySimpleType" />
		<xs:attribute name="lang" type="xs:anySimpleType" />
	</xs:complexType>
	<xs:element name="srep-search-opinion" type="srep-search-opinion" />
	<xs:complexType name="priority-date" mixed="true">
		<xs:attribute name="priority-valid" default="yes">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="priority-date" type="priority-date" />
	<xs:complexType name="compliance-unity-invention">
		<xs:sequence>
			<xs:element ref="p" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="unity-complied" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="compliance-unity-invention" type="compliance-unity-invention" />
	<xs:complexType name="invitation-pay-additional-fees">
		<xs:attribute name="unity-fee-code" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="1" />
					<xs:enumeration value="2" />
					<xs:enumeration value="3" />
					<xs:enumeration value="4" />
					<xs:enumeration value="5" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="invitation-pay-additional-fees" type="invitation-pay-additional-fees" />
	<xs:complexType name="annex-c-bis">
		<xs:attribute name="e-tables-missing" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="format-error" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="annex-c-bis" type="annex-c-bis" />
	<xs:complexType name="annex-c">
		<xs:attribute name="e-tables-missing" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="format-error" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="no-payment" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="annex-c" type="annex-c" />
	<xs:complexType name="priority-opinion">
		<xs:sequence>
			<xs:element ref="earlier-appl" minOccurs="0" />
			<xs:element ref="claim-invalid" minOccurs="0" />
			<xs:element ref="additional-info" minOccurs="0" />
		</xs:sequence>
		<xs:attribute name="varid" type="xs:anySimpleType" />
	</xs:complexType>
	<xs:element name="priority-opinion" type="priority-opinion" />
	<xs:complexType name="earlier-appl">
		<xs:attribute name="prio-code" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="1" />
					<xs:enumeration value="2" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="earlier-appl" type="earlier-appl" />
	<xs:complexType name="translation-of-appl">
		<xs:attribute name="translation" default="no">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="translation-purpose">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="international-type-search" />
					<xs:enumeration value="supplementary-international-search" />
					<xs:enumeration value="national-examination" />
					<xs:enumeration value="entry-regional-phase" />
					<xs:enumeration value="priority-document" />
					<xs:enumeration value="national-search" />
					<xs:enumeration value="opposition" />
					<xs:enumeration value="international-search" />
					<xs:enumeration value="international-publication" />
					<xs:enumeration value="international-preliminary-examination" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="lang" type="xs:anySimpleType" />
	</xs:complexType>
	<xs:element name="translation-of-appl" type="translation-of-appl" />
	<xs:complexType name="srep-info">
		<xs:choice>
			<xs:element ref="doc-page" maxOccurs="unbounded" />
			<xs:sequence>
				<xs:element ref="correspondence-address" minOccurs="0" />
				<xs:element ref="file-reference-id" minOccurs="0" />
				<xs:element ref="application-reference" maxOccurs="unbounded" />
				<xs:element ref="date-of-earliest-priority" minOccurs="0" />
				<xs:element ref="applicant-name" />
				<xs:element ref="srep-established" minOccurs="0" />
				<xs:element ref="srep-protest-fees" minOccurs="0" />
				<xs:element ref="srep-request-date" minOccurs="0" />
				<xs:element ref="srep-request-number" minOccurs="0" />
				<xs:element ref="srep-prior-art-docs" minOccurs="0"
					maxOccurs="unbounded" />
				<xs:element ref="srep-basis" minOccurs="0" />
				<xs:element ref="srep-claims-info" minOccurs="0" />
				<xs:element ref="srep-unity-of-invention" minOccurs="0" />
				<xs:element ref="srep-invention-title" />
				<xs:element ref="srep-abstract" />
				<xs:element ref="srep-figure-to-publish" />
				<xs:element ref="srep-info-admin" />
				<xs:element ref="srep-other-info" minOccurs="0" />
				<xs:element ref="text" minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
		</xs:choice>
		<xs:attribute name="total-page-count" type="xs:anySimpleType" />
	</xs:complexType>
	<xs:element name="srep-info" type="srep-info" />
	<xs:complexType name="srep-information" mixed="true">
		<xs:attribute name="srep-code" type="xs:anySimpleType" />
	</xs:complexType>
	<xs:element name="srep-information" type="srep-information" />
	<xs:complexType name="srep-figure-to-publish">
		<xs:sequence>
			<xs:element ref="figure-to-publish" minOccurs="0" />
		</xs:sequence>
		<xs:attribute name="figinfo" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="by-examiner" />
					<xs:enumeration value="by-applicant" />
					<xs:enumeration value="none" />
					<xs:enumeration value="none-suggested" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="srep-figure-to-publish" type="srep-figure-to-publish" />
	<xs:complexType name="srep-abstract">
		<xs:sequence>
			<xs:element ref="abstract" minOccurs="0" />
		</xs:sequence>
		<xs:attribute name="abs-approval" default="yes">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="srep-abstract" type="srep-abstract" />
	<xs:complexType name="srep-invention-title">
		<xs:sequence>
			<xs:element ref="invention-title" minOccurs="0" />
		</xs:sequence>
		<xs:attribute name="title-approval" default="yes">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="srep-invention-title" type="srep-invention-title" />
	<xs:complexType name="srep-protest-remark">
		<xs:attribute name="protest-remark" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="no-protest" />
					<xs:enumeration value="protest-fee-paid" />
					<xs:enumeration value="protest-fee-not-paid" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="srep-protest-remark" type="srep-protest-remark" />
	<xs:complexType name="basis-language-for-search">
		<xs:attribute name="translation" default="no">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="lang" type="xs:anySimpleType" />
	</xs:complexType>
	<xs:element name="basis-language-for-search" type="basis-language-for-search" />
	<xs:complexType name="sequence-listing-filing-time"
		mixed="true">
		<xs:attribute name="in-application" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="together-with-application" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="furnished-for-search" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="amendment">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="sequence-listing-filing-time" type="sequence-listing-filing-time" />
	<xs:complexType name="sequence-listing-material-format">
		<xs:attribute name="paper" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="electronic" default="yes">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="sequence-listing-material-format" type="sequence-listing-material-format" />
	<xs:complexType name="sequence-listing-material-type">
		<xs:attribute name="sequence-listing" default="yes">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="table" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="sequence-listing-material-type" type="sequence-listing-material-type" />
	<xs:complexType name="srep-prior-art-docs">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element ref="doc-page" />
			<xs:element ref="prior-art-xml-doc" />
		</xs:choice>
		<xs:attribute name="prior-art" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="srep-prior-art-docs" type="srep-prior-art-docs" />
	<xs:complexType name="prior-art-xml-doc">
		<xs:attribute name="id" type="xs:ID" />
		<xs:attribute name="file" type="xs:anySimpleType" use="required" />
		<xs:attribute name="lang" type="xs:anySimpleType" />
		<xs:attribute name="carriers">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="paper" />
					<xs:enumeration value="internal-electronic" />
					<xs:enumeration value="external-electronic" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="status" type="xs:anySimpleType" />
	</xs:complexType>
	<xs:element name="prior-art-xml-doc" type="prior-art-xml-doc" />
	<xs:complexType name="srep-protest-fees">
		<xs:attribute name="srep-protest" default="yes">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="srep-protest-fees" type="srep-protest-fees" />
	<xs:complexType name="srep-established">
		<xs:attribute name="srep-established" default="yes">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes" />
					<xs:enumeration value="no" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="srep-established" type="srep-established" />
	<xs:complexType name="drawings">
		<xs:choice>
			<xs:element ref="doc-page" maxOccurs="unbounded" />
			<xs:element ref="figure" maxOccurs="unbounded" />
		</xs:choice>
		<xs:attribute name="id" type="xs:ID" />
		<xs:attribute name="lang" type="xs:anySimpleType" />
		<xs:attribute name="status" type="xs:anySimpleType" />
	</xs:complexType>
	<xs:element name="drawings" type="drawings" />
	<xs:complexType name="figure">
		<xs:sequence>
			<xs:element ref="img" />
		</xs:sequence>
		<xs:attribute name="id" type="xs:ID" />
		<xs:attribute name="num" type="xs:anySimpleType" use="required" />
		<xs:attribute name="figure-labels" type="xs:anySimpleType" />
	</xs:complexType>
	<xs:element name="figure" type="figure" />
	<xs:complexType name="abstract">
		<xs:choice>
			<xs:element ref="doc-page" maxOccurs="unbounded" />
			<xs:sequence>
				<xs:element ref="abst-problem" />
				<xs:element ref="abst-solution" />
			</xs:sequence>
			<xs:element ref="p" maxOccurs="unbounded" />
		</xs:choice>
		<xs:attribute name="id" type="xs:ID" />
		<xs:attribute name="lang" type="xs:anySimpleType" />
		<xs:attribute name="status" type="xs:anySimpleType" />
	</xs:complexType>
	<xs:element name="abstract" type="abstract" />
	<xs:complexType name="abst-solution">
		<xs:sequence>
			<xs:element ref="p" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="id" type="xs:ID" />
	</xs:complexType>
	<xs:element name="abst-solution" type="abst-solution" />
	<xs:complexType name="abst-problem">
		<xs:sequence>
			<xs:element ref="p" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="id" type="xs:ID" />
	</xs:complexType>
	<xs:element name="abst-problem" type="abst-problem" />
	<xs:complexType name="amended-claims">
		<xs:choice>
			<xs:element ref="doc-page" maxOccurs="unbounded" />
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="heading" minOccurs="0" />
				<xs:element ref="claim" maxOccurs="unbounded" />
